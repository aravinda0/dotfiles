@theme "gruvbox-dark.rasi"


configuration {
  font: "SauceCodePro Nerd Font Mono 20";

  // Unbind conflicting defaults
  kb-remove-to-eol: "";
  kb-clear-line: "";
  kb-remove-char-forward: "";
  kb-move-char-forward: "";
  kb-move-char-back: "";

  kb-accept-entry: "Return";
  kb-row-down: "Control+j";
  kb-row-up: "Control+k";
  kb-remove-word-back: "Control+w";
  kb-page-next: "Control+f";
  kb-page-prev: "Control+b";
}
